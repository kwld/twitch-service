# Remote CLI mode settings for `twitch-eventsub-cli console --remote`
# Copy to `.cli.env` and fill values as needed.

# Required for remote mode
CLI_API_BASE_URL=https://your-api.example.com

# Required for service-auth actions (accessible bots, ws token listener)
CLI_SERVICE_CLIENT_ID=
CLI_SERVICE_CLIENT_SECRET=

# Required for admin-auth actions (list bots)
CLI_ADMIN_API_KEY=

# Optional (default: true)
CLI_VERIFY_TLS=true
